  "description": "High-performance Express.js backend API for Webstory news platform with multi-tier caching and AI commentary generation",
  "keywords": [
    "news",
    "api",
    "backend",
    "express",
    "vercel",
    "serverless",
    "mongodb",
    "ai",
    "caching"
  ],
  "author": "NukkadFoods",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/NukkadFoods/webstorybackend.git"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
